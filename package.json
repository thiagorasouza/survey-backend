{
  "name": "survey-backend",
  "version": "2.0.0",
  "description": "",
  "main": "src/main/index.js",
  "scripts": {
    "start": "node dist/main/index.js",
    "watch": "nodemon dist/main/index.js",
    "build": "rimraf dist && npx tsc -p tsconfig-build.json",
    "postbuild": "npx copyfiles -u 1 public/**/* dist/static",
    "debug": "nodemon -L --watch ./dist --inspect=0.0.0.0:9222 --nolazy dist/main/index.js",
    "up": "npm run build && docker-compose up -d",
    "down": "docker-compose down",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:watch": "npm test -- --watch",
    "test:unit": "npm test -- --config jest-unit-config.js",
    "test:integration": "npm test -- --config jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "test:coveralls": "npm run test:ci && coveralls < coverage/lcov.info"
  },
  "keywords": [],
  "author": "Thiago Souza",
  "license": "ISC",
  "devDependencies": {
    "@shelf/jest-mongodb": "^4.1.3",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.14",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^29.2.3",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.11.9",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "^13.7.10",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "copyfiles": "^2.4.1",
    "coveralls": "^3.1.1",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "git-commit-msg-linter": "^4.2.1",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "lint-staged": "^13.0.3",
    "mockdate": "^3.0.5",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@apollo/server": "^4.3.0",
    "bcrypt": "^5.1.0",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "graphql-scalars": "^1.20.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.12.0",
    "nocache": "^3.0.4",
    "nodemon": "^2.0.20",
    "swagger-ui-express": "^4.6.0",
    "validator": "^13.7.0"
  },
  "engines": {
    "node": "16.x"
  }
}
